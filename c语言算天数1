#include<stdio.h>
int main(void)
{
	int a, b, c, d, sum; //定义四个整形变量
	scanf_s("%d%d", &a, &d);//从键盘录入三个整数
    if (a % 4 == 0)//判断a年份是不是闰年
    {
		b = 2020 - a;//用2020年减去你出生的年份，得出b年
	    c = b / 4 + 1;//判断b年中有几次闰年，把次数赋值给c
	    b = b - c;//判断不是闰年的有几年，重新赋值给b
	    b *= 365;//不是闰年的共有多少天
	    c *= 366;//是闰年的共有多少天
		sum = b + c;//共有多少天
	}
	else  if (a % 4 == 1)//判断a年份是不是闰年的上一年或者下一年
	{
		b = 2020 - a;//用2020年减去你出生的年份，得出b年
		c = b / 4 + 1;//判断b年中有几次闰年，把次数赋值给c
		b = b - c;//判断不是闰年的有几年，重新赋值给b
		c *= 366;//不是闰年的共有多少天
		b *= 365;//是闰年的共有多少天
		sum = b + c;//共有多少天
	}
	else  if (a % 4 == 2)//判断a年份是不是闰年的上二年或者下二年
	{
		b = 2020 - a;//用2020年减去你出生的年份，得出b年
		c = b / 4 + 1;//判断b年中有几次闰年，把次数赋值给c
		b = b - c;//判断不是闰年的有几年，重新赋值给b
		c *= 366;//不是闰年的共有多少天
		b *= 365;//是闰年的共有多少天
		sum = b + c;//共有多少天
	}
	else  if (a % 4 == 3)//判断a年份是不是闰年的上三年或者下三年
	{
		b = 2020 - a;//用2020年减去你出生的年份，得出b年
		c = b / 4 + 1;//判断b年中有几次闰年，把次数赋值给c
		b = b - c;//判断不是闰年的有几年，重新赋值给b
		c *= 366;//不是闰年的共有多少天
		b *= 365;//是闰年的共有多少天
		sum = b + c;//共有多少天			
	}
